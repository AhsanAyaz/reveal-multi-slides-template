{
  "name": "cwa-slides",
  "version": "4.4.0",
  "description": "",
  "homepage": "https://revealjs.com",
  "subdomain": "revealjs",
  "main": "dist/reveal.js",
  "module": "dist/reveal.esm.js",
  "license": "MIT",
  "scripts": {
    "test": "jest && gulp test",
    "dev": "npm run extract && webpack serve",
    "build": "npm run extract && webpack --mode production",
    "package": "gulp package",
    "build-content": "npm run extract && gulp build-content",
    "deploy": "gulp deploy",
    "predeploy": "npm run build-content",
    "extract": "node scripts/extractSlideData.js",
    "lint": "eslint ./js/**/* --fix",
    "serve:prod": "npm run build && npx http-server ./dist"
  },
  "author": {
    "name": "Muhammad Ahsan Ayaz",
    "email": "ahsan.ubitian@gmail.com",
    "web": "https://codewithahsan.dev"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ahsanayaz/slides.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "reveal",
    "slides",
    "presentation"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "^7.24.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-html-import-to-string": "0.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.12.1",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-webpack-plugin": "^4.1.0",
    "fitty": "^2.3.0",
    "gh-pages": "^4.0.0",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-connect": "^5.7.0",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.9",
    "gulp-tap": "^2.0.0",
    "gulp-zip": "^4.2.0",
    "highlight.js": "^10.0.3",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "marked": "^4.0.12",
    "mini-css-extract-plugin": "^2.8.1",
    "node-qunit-puppeteer": "^2.1.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.8.0",
    "puppeteer": "^22.6.4",
    "qunit": "^2.20.1",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "yargs": "^15.1.0"
  },
  "browserslist": "> 2%, not dead",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "requireConfigFile": false
    },
    "globals": {
      "module": false,
      "console": false,
      "unescape": false,
      "define": false,
      "exports": false
    },
    "rules": {
      "curly": 0,
      "eqeqeq": 2,
      "wrap-iife": [
        2,
        "any"
      ],
      "no-use-before-define": [
        2,
        {
          "functions": false
        }
      ],
      "new-cap": 2,
      "no-caller": 2,
      "dot-notation": 0,
      "no-eq-null": 2,
      "no-unused-expressions": 0
    }
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
}
